DEBUG=<%= @debug %>
DATABASE_URL=<%= @database_url %>
DJANGO_SETTINGS_MODULE=<%= @django_settings_module %>
HIPCHAT_URL=https://api.hipchat.com/v1/rooms/message
LOG_FILE=/dev/null
PORT=<%= @port %>
VENV=<%= @virtualenv_dir %>

# You shouldn't need to change anything above this line
# Django admin email
ADMIN_EMAIL=<%= @admin_email %>
CABOT_FROM_EMAIL=<%= @from_email %>

# URL of calendar to synchronise rota with
CALENDAR_ICAL_URL=<%= @ical_url %>

# Django settings
CELERY_BROKER_URL=<%= @celery_broker_url %>
DJANGO_SECRET_KEY=<%= @django_secret_key %>

# Hostname of your Graphite server instance
GRAPHITE_API=<%= @graphite_api_url %>
GRAPHITE_USER=<%= @graphite_username %>
GRAPHITE_PASS=<%= @graphite_password %>

# Hipchat integration
HIPCHAT_ALERT_ROOM=<%= @hipchat_room_id %>
HIPCHAT_API_KEY=<%= @hipchat_api_key %>

# Jenkins integration
JENKINS_API=<%= @jenkins_api_url %>
JENKINS_USER=<%= @jenkins_username %>
JENKINS_PASS=<%= @jenkins_password %>

# SMTP settings
SES_HOST=<%= @smtp_host %>
SES_USER=<%= @smtp_username %>
SES_PASS=<%= @smtp_password %>
SES_PORT=<%= @smtp_port %>

# Twilio integration for SMS and telephone alerts
TWILIO_ACCOUNT_SID=<%= @twilio_account_sid %>
TWILIO_AUTH_TOKEN=<%= @twilio_auth_token %>
TWILIO_OUTGOING_NUMBER=<%= @twilio_outgoing_number %>

# Used for pointing links back in alerts etc.
WWW_HTTP_HOST=<%= @www_http_host %>
WWW_SCHEME=<%= @www_scheme %>
